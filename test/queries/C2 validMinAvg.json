            "COLUMNS": [
                "courses_dept",
                "MINavg"
            ],
            "ORDER": "MINavg"
        },
        "TRANSFORMATIONS": {
            "GROUP": [
                "courses_dept"
            ],
            "APPLY": [
                {
                    "MINavg": {
                        "MIN": "courses_avg"
                    }
                }
            ]
        }
